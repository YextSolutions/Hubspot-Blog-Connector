{
  "$id": "ce_helpArticle",
  "$schema": "https://schema.yext.com/config/km/field-presentation/v1",
  "entityType": "helpArticle",
  "displayName": "Help Article",
  "presentationEntries": [
    {
      "displayName": "Content",
      "entryType": "DIVIDER"
    },
    {
      "displayName": "Core Information",
      "entryType": "SECTION",
      "fields": [
        {
          "id": "name",
          "visibleOnAdd": true
        },
        {
          "id": "entityId",
          "visibleOnAdd": true
        }
      ]
    },
    {
      "displayName": "Article Details",
      "entryType": "SECTION",
      "fields": [
        {
          "id": "body"
        },
        {
          "id": "shortDescription"
        },
        {
          "id": "hubspotblogs_featuredImage"
        }
      ]
    },
    {
      "displayName": "Additional Attributes",
      "entryType": "SECTION",
      "fields": [
        {
          "id": "landingPageUrl"
        },
        {
          "id": "keywords"
        },
        {
          "id": "voteCount"
        },
        {
          "id": "voteSum"
        },
        {
          "id": "promoted"
        },
        {
          "id": "hubspotblogs_authorName"
        }
      ]
    },
    {
      "displayName": "Settings",
      "entryType": "DIVIDER"
    },
    {
      "defaultCollapsed": true,
      "displayName": "Internal Use Only",
      "entryType": "SECTION",
      "fields": [
        {
          "id": "folder"
        },
        {
          "id": "labels"
        },
        {
          "id": "timezone"
        }
      ]
    },
    {
      "defaultCollapsed": true,
      "displayName": "Knowledge Assistant",
      "entryType": "SECTION",
      "fields": [
        {
          "id": "primaryConversationContact"
        },
        {
          "id": "nudgeEnabled"
        }
      ]
    }
  ]
}